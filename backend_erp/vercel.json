{
  "builds": [
    {
      "src": "backend_erp/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9.0" }
    }
  ],
  "routes": [
    {
      "src": "/admin/(.*)",
      "dest": "backend_erp/wsgi.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "backend_erp/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "backend_erp/wsgi.py"
    }
  ]
}
